{
    "name": "ssionn/github-forge-laravel",
    "description": "A GitHub REST API client for Laravel",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/http": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/pagination": "^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ssionn\\GithubForgeLaravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ssionn",
            "email": "casperkizewski@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "archive": {
        "exclude": [
            "/tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ssionn\\GithubForgeLaravel\\GithubForgeServiceProvider"
            ],
            "aliases": {
                "GithubForge": "Ssionn\\GithubForgeLaravel\\Facades\\GithubForge"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2"
    }
}
